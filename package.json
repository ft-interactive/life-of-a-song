{
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "axios": "^0.27.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^4.1.0",
    "css-loader": "^6.10.0",
    "cssnano": "^4.1.10",
    "csv-loader": "^3.0.2",
    "d3-time-format": "^2.2.3",
    "dotenv": "^6.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.5",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^6.2.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^7.0.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "jsdom": "^20.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "mocha": "^8.1.3",
    "pa11y": "^5.3.0",
    "postcss-import": "^16.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "raw-loader": "^0.5.1",
    "react-is": "^16.13.1",
    "sass": "^1.24.4",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.2.4"
  },
  "optionalDependencies": {
    "@financial-times/g-deploy": "3.1.0",
    "react-snap": "^1.23.0"
  },
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && touch dist/index.html && webpack --env=production",
    "postbuild": "react-snap && rm dist/index.html",
    "a11y": "pa11y $(g-deploy --get-commit-url)",
    "a11y:preview": "pa11y $(g-deploy --preview --get-commit-url)",
    "a11y:local": "pa11y localhost:8080",
    "deploy": "g-deploy --assets-prefix=https://ig.ft.com/v2/__assets/",
    "test": "mocha ./test/**/*.spec.js",
    "start": "webpack serve --env=development",
    "lint": "eslint app/**/*.js test/**/*.js",
    "lint:fix": "eslint --fix app/**/*.js test/**/*.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@financial-times/g-audio": "^2.0.2",
    "@financial-times/g-components": "^9.4.1",
    "@financial-times/o-colors": "^6.4.0",
    "@financial-times/o-grid": "^6.1.0",
    "bertha-client": "^4.0.1",
    "cheerio": "^1.0.0-rc.12",
    "core-js": "^3.35.1",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "structured-google-docs-client": "^0.0.3"
  },
  "reactSnap": {
    "source": "dist",
    "skipThirdPartyRequests": true
  }
}
